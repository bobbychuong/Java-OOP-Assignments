
public class BankAccountTest {

	public static void main(String[] args) {
		BankAccount ba = new BankAccount();
		ba.BankAccount();
		ba.RandAccNumber();
		ba.getChecking();
		ba.getSaving();
		ba.depositChecking(25);
		ba.depositSaving(100);
		ba.withdrawChecking();
		ba.withdrawSaving();
		ba.totalAmount();
	}

}
